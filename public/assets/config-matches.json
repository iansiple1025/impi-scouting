{
  "heading": "IMPI Scouting - 2024",
  "logo": "logo.png",
  "pages": [
    {
      "name": "Setup",
      "widgets": [
        {
          "name": "Scouter Name",
          "type": "text",
          "validation": {
            "comparison": "greater",
            "value": 0
          }
        },
        {
          "name": "Click Starting Position",
          "type": "positions",
          "file": "field.png"
        }
      ]
    },
    {
      "name": "Autonomous",
      "widgets": [
        {
          "name": "Passed white line",
          "type": "checkbox"
        },
        {
          "type": "spacer",
          "height": 20
        },
        {
          "name": "Amp Notes",
          "type": "spinbox",
          "prefix": "Auto"
        },
        {
          "name": "Speaker Notes",
          "type": "spinbox",
          "prefix": "Auto"
        }
      ]
    },
    {
      "name": "Teleop",
      "widgets": [
        {
          "name": "Scoring",
          "type": "heading",
          "colspan": 5,
          "align": "center"
        },
        {
          "name": "Speaker Notes",
          "type": "spinbox"
        },
        {
          "name": "Amp Notes",
          "type": "spinbox"
        },
        {
          "type": "spacer",
          "height": 20
        },
        {
          "name": "Defense Rating",
          "type": "dropdown",
          "options": [
            "0 (Not Played)",
            "1",
            "2",
            "3",
            "4",
            "5 (Very Effective)"
          ],
          "col": 2
        },
        {
          "name": "Tipped",
          "type": "checkbox",
          "col": 2
        }
      ]
    },
    {
      "name": "Endgame",
      "widgets": [
        {
          "name": "End Location",
          "type": "dropdown",
          "options": [
            "Onstage",
            "Parked",
            "None"
          ],
          "default": 3
        },
        {
          "name": "Scored trap note",
          "type": "checkbox"

        },
        {
          "name": "Comments",
          "type": "textarea"
        },
        {
          "name": "Stage Positions Diagram",
          "type": "heading",
          "colspan": 2,
          "align": "center"
        },
        {
          "type": "picture",
          "file": "stage.png",
          "colspan": 2,
          "height": 250
        }
      ]
    }
  ]
}